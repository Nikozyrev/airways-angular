{
  "useTabs": false,
  "tabWidth": 2,
  "printWidth": 80,
  "semi": true,
  "endOfLine": "auto",
  "bracketSpacing": true,
  "trailingComma": "es5",
  "htmlWhitespaceSensitivity": "ignore",
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "angular"
      }
    }
  ]
}
